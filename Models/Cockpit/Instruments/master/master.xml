            <command>nasal</command>
            <script>
                if (getprop("/controls/lighting/instruments-norm")== 0.0){
                setprop("/controls/lighting/instruments-norm", 0.5);}else{setprop("/controls/lighting/instruments-norm", 0.0);}
		        </script>
        </binding>	
        <binding>
          <command>property-toggle</command>

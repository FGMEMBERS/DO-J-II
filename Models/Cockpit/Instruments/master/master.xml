  <animation>
    <type>material</type>
      <object-name>nightHelp</object-name>
      <emission>
        <red>0.5</red>
        <green>0.7</green>
        <blue>0.2</blue>
      </emission>
  </animation>

  <animation>
      <type>select</type>
      <object-name>nightHelp</object-name>
      <condition>
        <less-than>
          <property>sim/multiplay/generic/float[3]</property>
          <value>0.5</value>
        </less-than>
      </condition>
  </animation>

  <animation>
    <type>material</type>
      <object-name>lampOn</object-name>
    <emission>
      <red>1</red>
      <green>1</green>
      <blue>0</blue>
      <factor-prop>/systems/electrical/volts</factor-prop>
    </emission>
  </animation>

    <!-- Main Control -->
    <animation>
        <type>select</type>
        <object-name>lampOff</object-name>
        <condition>
          <or>
            <less-than>
              <property>/systems/electrical/volts</property>
              <value>6</value>
            </less-than>
            <not>
              <property>/controls/electric/battery-switch</property>
            </not>
          </or>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>lampOn</object-name>
        <condition>
          <and>
            <greater-than>
              <property>/systems/electrical/volts</property>
              <value>6</value>
            </greater-than>            
            <property>/controls/electric/battery-switch</property>
          </and>
        </condition>
    </animation>


<!--  MAGNETO - KNOB -->
   <animation>
        <type>rotate</type>
        <object-name>magnetoKnob.001</object-name>
        <property>/controls/engines/engine/magnetos</property>
        <interpolation>
          <entry><ind>0</ind><dep>-88</dep></entry>
          <entry><ind>1</ind><dep>-43</dep></entry>
          <entry><ind>2</ind><dep> 0</dep></entry>
          <entry><ind>3</ind><dep> 43</dep></entry>
        </interpolation>
        <axis>
            <x>1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </axis>
        <center>
            <x-m> 0.0</x-m>
            <y-m>-0.0075</y-m>
            <z-m> 0.0098</z-m>
        </center>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>magnetoKnob.001</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
              <command>property-adjust</command>
              <property>/controls/engines/engine/magnetos</property>
              <step>1</step>
              <min>0</min>
              <max>3</max>
              <wrap>0</wrap>
            </binding>
            <binding>
              <command>property-toggle</command>
              <property>/sim/sound/switch</property>
            </binding>
        </action>

        <action>
            <button>1</button>
            <interval-sec>0.2</interval-sec>
            <binding>
              <command>property-adjust</command>
              <property>/controls/engines/engine/magnetos</property>
              <step>-1</step>
              <min>0</min>
              <max>3</max>
              <wrap>0</wrap>
            </binding>
            <binding>
              <command>property-toggle</command>
              <property>/sim/sound/switch</property>
            </binding>
        </action>
    </animation>


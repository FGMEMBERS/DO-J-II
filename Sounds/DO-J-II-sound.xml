    <creak-positive>
     <name>creak-positive</name>
     <mode>once</mode>
     <path>Aircraft/DO-J-II/Sounds/towel.wav</path>
     <condition>
      <or>
        <greater-than>
         <property>/accelerations/pilot-g</property>
         <value>1.3</value>
        </greater-than>
        <property>/controls/special/catapult-carrier-crane/hook-locked</property>
      </or>
     </condition>
     <volume>
      <property>/accelerations/pilot-g</property>
      <factor>0.3</factor>
      <max>1</max>
     </volume>
    </creak-positive>

    <whoosh-negative>
     <name>whoosh-negative</name>
     <mode>once</mode>

<PropertyList>

  <fx>
    <crank>
        <name>cranking</name>
        <mode>looped</mode>
        <path>Aircraft/DO-J-II/Sounds/starter.wav</path>
        <condition>
            <property>/engines/engine[0]/cranking</property>
        </condition>
        <volume>
            <property>/engines/engine[0]/cranking</property>
            <offset>0.5</offset>
        </volume>
        <position>
            <x>-2.0</x>
            <y>0</y>
            <z>0</z>
        </position>
        <reference-dist>5.0</reference-dist>
        <max-dist>10.0</max-dist>
    </crank>

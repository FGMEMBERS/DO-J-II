    <wind>
        <name>wind</name>
        <mode>looped</mode>
        <path>Sounds/wind.wav</path>
        <property>velocities/airspeed-kt</property>
        <volume>
            <property>position/altitude-ft</property>
            <factor>-0.000015</factor>
            <offset>1.0</offset>
            <min>0.1</min>
            <max>1.0</max>
        </volume>
        <volume>
            <property>velocities/airspeed-kt</property>
            <factor>0.0015</factor>
            <min>0.03</min>
            <max>0.25</max>
        </volume>
        <pitch>
            <property>velocities/airspeed-kt</property>
            <factor>0.01</factor>
            <offset>1.0</offset>
        </pitch>
        <position>
            <x>0</x>
            <y>0</y>
            <z>0</z>
        </position>
        <max-dist>2</max-dist>
        <reference-dist>1</reference-dist>
    </wind>

 <!--  Toggle Switches  -->     
    <switch>
      <name>click on</name>
      <mode>once</mode>
      <path>Sounds/click.wav</path>
      <condition>
        <property>/sim/sound/switch</property>
      </condition>
      <volume>
        <offset> 0.7 </offset>
      </volume>
      <position>
        <x> -0.5 </x>
        <y>  0.0 </y>
        <z>  0.0 </z>
      </position>
      <reference-dist> 2.0 </reference-dist>
      <max-dist> 4.0 </max-dist>
    </switch>

    <switch>
      <name>click off</name>
      <mode>once</mode>
      <path>Sounds/click.wav</path>
      <condition>
        <not>
          <property>/sim/sound/switch</property>
        </not>
      </condition>
      <volume>
        <offset> 0.7 </offset>
      </volume>
      <position>
        <x> -0.5 </x>
        <y>  0.0 </y>
        <z>  0.0 </z>
      </position>
      <reference-dist> 2.0 </reference-dist>
      <max-dist> 4.0 </max-dist>
    </switch>

    <rain>
      <name>rain</name>
      <mode>looped</mode>
      <path>Aircraft/DO-J-II/Sounds/rain_canopy.wav</path>
      <condition>
        <greater-than>
          <property>/environment/metar/rain-norm</property>
          <value>0.2</value>
        </greater-than>
      </condition>
      <volume>
        <property>/environment/metar/rain-norm</property>
        <factor>1.00</factor>
        <offset>0.00</offset>
        <min>0.4</min>
        <max>0.9</max>
      </volume>
      <pitch>
        <property>/velocities/airspeed-kt</property>
        <factor>0.001</factor>
        <offset>1.0</offset>
      </pitch>
    </rain>

   <!-- for radio direction finder -->
    <rdfN>
     <name>NAV-sound</name>
     <mode>looped</mode>
     <path>Aircraft/DO-J-II/Sounds/rdf.wav</path>
     <condition>
        <or>
          <and>
            <not>
              <property>/dual-control/active</property>
            </not>
            <property>/dual-control/i-am-the-pilot</property>           
          </and>
          <and>
            <property>/dual-control/active</property>
            <not>
              <property>/dual-control/i-am-the-pilot</property>
            </not>           
          </and>
        </or>
      <and>
        <property>/instrumentation/rdf/power-on</property>
        <property>/instrumentation/nav/power-btn</property>
        <property>/instrumentation/rdf/headset/position</property>        
        <equals>
           <property>/instrumentation/rdf/frequency-select-knob</property>
           <value>1</value>
        </equals>
        <or>
         <equals>
           <property>/instrumentation/nav/in-range</property>
           <value>1</value>
         </equals>
         <equals>
           <property>/instrumentation/rdf/cat-ship-in-range</property>
           <value>1</value>
         </equals>
        </or>
       </and>
     </condition>
      <pitch>
          <property>/instrumentation/nav/volume</property>
          <factor>1</factor>
      </pitch>
      <volume>
        <property>/instrumentation/nav/volume</property>
        <factor>1</factor>
      </volume>
    </rdfN>

    <rdfA>
     <name>ADF-sound</name>
     <mode>looped</mode>
     <path>Aircraft/DO-J-II/Sounds/rdf.wav</path>
     <condition>
      <or>
        <and>
          <not>
            <property>/dual-control/active</property>
          </not>
          <property>/dual-control/i-am-the-pilot</property>           
        </and>
        <and>
          <property>/dual-control/active</property>
          <not>
            <property>/dual-control/i-am-the-pilot</property>
          </not>           
        </and>
      </or>
      <and>
        <property>/instrumentation/rdf/power-on</property>
        <property>/instrumentation/adf/power-btn</property>
        <property>/instrumentation/adf/in-range</property>
        <property>/instrumentation/rdf/headset/position</property>
        <equals>
           <property>/instrumentation/rdf/frequency-select-knob</property>
           <value>0</value>
        </equals>
      </and>
     </condition>
      <pitch>
          <property>/instrumentation/adf/volume-norm</property>
          <factor>1</factor>
      </pitch>
      <volume>
        <property>/instrumentation/adf/volume-norm</property>
        <factor>1</factor>
      </volume>
    </rdfA>

    <intro1>
        <name>GambiaSound</name>
        <mode>once</mode>
        <path>Aircraft/DO-J-II/Sounds/intro_gambia.wav</path>
        <condition>
          <equals>
            <property>/sim/airport/closest-airport-id</property>
            <value type="string">GBYD</value>
          </equals>
        </condition>
        <volume>
				  <factor>1</factor>
        </volume>
        <position>
            <x>-6.0</x>
            <y>0</y>
            <z>0</z>
        </position>
        <reference-dist>300.0</reference-dist>
        <max-dist>1000.0</max-dist>
    </intro1>

    <intro2>
        <name>BrazilSound</name>
        <mode>once</mode>
        <path>Aircraft/DO-J-II/Sounds/intro_brazilian.wav</path>
        <condition>
          <equals>
            <property>/sim/airport/closest-airport-id</property>
            <value type="string">SBFN</value>
          </equals>
        </condition>
        <volume>
				  <factor>1</factor>
        </volume>
        <position>
            <x>-6.0</x>
            <y>0</y>
            <z>0</z>
        </position>
        <reference-dist>300.0</reference-dist>
        <max-dist>1000.0</max-dist>
    </intro2>

    <catelevator>
        <name>CatElevator</name>
        <mode>looped</mode>
        <path>Aircraft/DO-J-II/Sounds/cat_elevator.wav</path>
        <condition>
          <and>
            <greater-than>
              <property>/controls/special/catapult-carrier-crane/cat-elevator-westfalen</property>
              <value>0.0</value>
            </greater-than>
            <less-than>
              <property>/controls/special/catapult-carrier-crane/cat-elevator-westfalen</property>
              <value>1.65</value>

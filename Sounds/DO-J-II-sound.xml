        <not>
         <property>/controls/special/catapult-carrier-crane/hook-locked</property>
        </not>
      </or>
     </condition>
     <volume>
      <property>/orientation/pitch-rate-degps</property>
      <factor>-0.02</factor>
      <max>1</max>

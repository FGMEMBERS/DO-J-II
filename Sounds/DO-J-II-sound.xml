          </and>
        </condition>
        <volume>
				  <factor>1</factor>
        </volume>
        <position>
            <x>-6.0</x>
            <y>0</y>
            <z>0</z>
        </position>
        <reference-dist>300.0</reference-dist>
        <max-dist>1000.0</max-dist>
    </catelevator>

  </fx>

</PropertyList>

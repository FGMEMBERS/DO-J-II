<PropertyList>
	<path>kranRaeder.ac</path>

	<animation>
		<object-name>craneWheel</object-name>
		<object-name>craneWheel2</object-name>
		<object-name>smallTowel</object-name>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>craneWheel</object-name>
		<property>surface-positions/crane-stand</property>
		<factor>1000</factor>
		<center>
			<x-m>-5.419</x-m>
			<y-m>0.0</y-m>
			<z-m>7.847</z-m>
		</center>

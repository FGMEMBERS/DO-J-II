	<animation>
		<type>rotate</type>
		<object-name>craneWheel2</object-name>
		<property>surface-positions/crane-stand</property>
		<factor>1000</factor>
		<center>
			<x-m>-14.204</x-m>
			<y-m>0.0</y-m>
			<z-m>8.686</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z> 
		</axis>
	</animation>

</PropertyList>
